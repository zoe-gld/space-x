<div class="index-container">
  <div class="row">
    <% @spaces.each do |space| %>
        <div class="col-lg-4 col-md-6 col-sm-12">
          <div class="card-trip">
            <img src="https://images.unsplash.com/photo-1553969536-7abe08c6132d?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1650&q=80" alt="">

            <div class="card-text">
              <div class="card-info">
                <h2><%= link_to(space.name, space_path(space)) %></h2>
                 <p><%= space.user.first_name %> <%= space.user.last_name %></p>
              </div>
              <h3>üìç<%= space.address %></h3>
            </div>


          </div>
        </div>
    <% end %>
  </div>
</div>
