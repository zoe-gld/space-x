<%= simple_form_for @user do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :phone_number %>
  <%= f.input :email %>
  <%= f.input :avatar %>
  <%= f.input :avatar_cache, as: :hidden %>
  <%= f.submit class: 'btn btn-primary mb-2' %>
<% end %>

<%= link_to("Back", my_account_path) %>
